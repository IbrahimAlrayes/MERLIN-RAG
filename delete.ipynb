    "import requests\n",
    "from datetime import datetime, timedelta\n",
    "\n",
    "TITLE = \"Agentic_AI\"  # Wikipedia page title (URL-encoded with underscores)\n",
    "BASE_URL = \"https://en.wikipedia.org/w/api.php\"\n",
    "\n",
    "def get_page_info(title):\n",
    "    \"\"\"Returns article size and revision count\"\"\"\n",
    "    params = {\n",
    "        \"action\": \"query\",\n",
    "        \"prop\": \"info|revisions|contributors|categories|images|extlinks\",\n",
    "        \"titles\": title,\n",
    "        \"format\": \"json\",\n",
    "        \"rvprop\": \"ids|timestamp|user\",\n",
    "        \"rvlimit\": \"max\",\n",
    "        \"cllimit\": \"max\",\n",
    "        \"imlimit\": \"max\",\n",
    "        \"ellimit\": \"max\",\n",
    "    }\n",
    "    r = requests.get(BASE_URL, params=params)\n",
    "    data = r.json()\n",
    "    page = next(iter(data[\"query\"][\"pages\"].values()))\n",
    "    return {\n",
    "        \"article_size_bytes\": page.get(\"length\"),\n",
    "        \"revision_count\": len(page.get(\"revisions\", [])),\n",
    "        \"unique_editors\": len({rev[\"user\"] for rev in page.get(\"revisions\", [])}),\n",
    "        \"category_count\": len(page.get(\"categories\", [])) if \"categories\" in page else 0,\n",
    "        \"image_count\": len(page.get(\"images\", [])) if \"images\" in page else 0,\n",
    "        \"external_links\": len(page.get(\"extlinks\", [])) if \"extlinks\" in page else 0,\n",
    "    }\n",
    "\n",
    "def get_backlinks(title):\n",
    "    \"\"\"Count backlinks (articles linking to this one).\"\"\"\n",
    "    params = {\n",
    "        \"action\": \"query\",\n",
    "        \"list\": \"backlinks\",\n",
    "        \"bltitle\": title,\n",
    "        \"bllimit\": \"max\",\n",
    "        \"format\": \"json\",\n",
    "    }\n",
    "    r = requests.get(BASE_URL, params=params)\n",
    "    return len(r.json()[\"query\"][\"backlinks\"])\n",
    "def safe_get_json(url, params=None):\n",
    "    r = requests.get(url, params=params)\n",
    "    if r.status_code != 200:\n",
    "        raise RuntimeError(f\"HTTP {r.status_code} from {r.url}\")\n",
    "    if not r.text.strip():  # empty body\n",
    "        raise RuntimeError(f\"Empty response from {r.url}\")\n",
    "    try:\n",
    "        return r.json()\n",
    "    except Exception as e:\n",
    "        print(\"Raw response:\", r.text[:500])  # peek at first 500 chars\n",
    "        raise RuntimeError(f\"Invalid JSON from {r.url}\") from e\n",
    "\n",
    "\n",
    "def get_pageviews(title, days=90):\n",
    "    end = datetime.utcnow().date()\n",
    "    start = end - timedelta(days=days)\n",
    "    url = f\"https://wikimedia.org/api/rest_v1/metrics/pageviews/per-article/en.wikipedia/all-access/all-agents/{title}/daily/{start:%Y%m%d}/{end:%Y%m%d}\"\n",
    "    headers = {\"User-Agent\": \"AgenticAI/1.0 (https://github.com/yourusername)\"}\n",
    "    r = requests.get(url, headers=headers)\n",
    "    if r.status_code != 200:\n",
    "        print(\"Response text:\", r.text)\n",
    "        raise RuntimeError(f\"Error {r.status_code} from Wikimedia\")\n",
    "    data = r.json()\n",
    "    return sum(item[\"views\"] for item in data.get(\"items\", []))\n",
    "\n",
    "\n",
    "# Combine all metrics\n",
    "metrics = {\n",
    "    **get_page_info(TITLE),\n",
    "    \"wikipedia_backlinks\": get_backlinks(TITLE),\n",
    "    \"wikipedia_pageviews_90d\": get_pageviews(TITLE),\n",
    "}\n",
    "\n",
    "print(metrics)\n"
   ]
