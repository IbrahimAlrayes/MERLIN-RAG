    "import requests, time\n",
    "from datetime import datetime, timedelta\n",
    "from typing import Dict, Any, Optional\n",
    "import json \n",
    "# Base APIs\n",
    "WIKIDATA_API = \"https://www.wikidata.org/w/api.php\"\n",
    "WIKI_API = \"https://en.wikipedia.org/w/api.php\"\n",
    "\n",
    "# Config\n",
    "REQUEST_DELAY = 0.2\n",
    "MAX_RETRIES = 3\n",
    "HEADERS = {\"User-Agent\": \"AgenticAI-Metrics/1.0 (https://github.com/yourname)\"}\n",
    "\n",
    "\n",
    "def call_api_with_retry(url: str, params: Dict[str, Any]) -> Optional[Dict[str, Any]]:\n",
    "    \"\"\"Universal GET wrapper with retry + delay + JSON safety.\"\"\"\n",
    "    for attempt in range(MAX_RETRIES):\n",
    "        try:\n",
    "            time.sleep(REQUEST_DELAY)\n",
    "            r = requests.get(url, params=params, headers=HEADERS)\n",
    "            r.raise_for_status()\n",
    "            return r.json()\n",
    "        except Exception as e:\n",
    "            if attempt == MAX_RETRIES - 1:\n",
    "                print(f\"[WARN] API failed after {MAX_RETRIES} retries: {e}\")\n",
    "                return None\n",
    "            time.sleep(2 ** attempt)  # exponential backoff\n",
    "\n",
    "\n",
    "def get_pageviews_90d(title: str) -> int:\n",
    "    \"\"\"Total English-Wikipedia pageviews in the last 90 days.\"\"\"\n",
    "    if not title:\n",
    "        return 0\n",
    "    end = datetime.utcnow().date()\n",
    "    start = end - timedelta(days=90)\n",
    "    url = (\n",
    "        f\"https://wikimedia.org/api/rest_v1/metrics/pageviews/per-article/\"\n",
    "        f\"en.wikipedia.org/all-access/all-agents/{title.replace(' ', '_')}/\"\n",
    "        f\"daily/{start:%Y%m%d}/{end:%Y%m%d}\"\n",
    "    )\n",
    "    data = call_api_with_retry(url, {})\n",
    "    if not data or \"items\" not in data:\n",
    "        return 0\n",
    "    return sum(i.get(\"views\", 0) for i in data[\"items\"])\n",
    "\n",
    "\n",
    "def get_wikipedia_structural_metrics(title: str) -> Dict[str, Any]:\n",
    "    \"\"\"Query revision/edit/category/image/link counts for one article.\"\"\"\n",
    "    if not title:\n",
    "        return {}\n",
    "    params = {\n",
    "        \"action\": \"query\",\n",
    "        \"prop\": \"info|revisions|contributors|categories|images|extlinks\",\n",
    "        \"titles\": title,\n",
    "        \"format\": \"json\",\n",
    "        \"rvprop\": \"user\",\n",
    "        \"rvlimit\": \"max\",\n",
    "        \"cllimit\": \"max\",\n",
    "        \"imlimit\": \"max\",\n",
    "        \"ellimit\": \"max\",\n",
    "    }\n",
    "    data = call_api_with_retry(WIKI_API, params)\n",
    "    if not data or \"query\" not in data:\n",
    "        return {}\n",
    "    page = next(iter(data[\"query\"][\"pages\"].values()))\n",
    "    revisions = page.get(\"revisions\", [])\n",
    "    editors = {rev.get(\"user\") for rev in revisions if \"user\" in rev}\n",
    "    return {\n",
    "        \"article_size_bytes\": page.get(\"length\"),\n",
    "        \"revision_count\": len(revisions),\n",
    "        \"unique_editors\": len(editors),\n",
    "        \"category_count\": len(page.get(\"categories\", [])) if \"categories\" in page else 0,\n",
    "        \"image_count\": len(page.get(\"images\", [])) if \"images\" in page else 0,\n",
    "        \"external_links\": len(page.get(\"extlinks\", [])) if \"extlinks\" in page else 0,\n",
    "    }\n",
    "\n",
    "\n",
    "def count_backlinks(title: str, threshold: int = 3000) -> int:\n",
    "    \"\"\"Count how many other pages link to this one.\"\"\"\n",
    "    if not title:\n",
    "        return 0\n",
    "    params = {\"action\": \"query\", \"list\": \"backlinks\", \"bltitle\": title,\n",
    "              \"bllimit\": \"max\", \"format\": \"json\"}\n",
    "    total = 0\n",
    "    while True:\n",
    "        data = call_api_with_retry(WIKI_API, params)\n",
    "        if not data:\n",
    "            break\n",
    "        links = data.get(\"query\", {}).get(\"backlinks\", [])\n",
    "        total += len(links)\n",
    "        if total >= threshold:\n",
    "            return total\n",
    "        if \"continue\" in data:\n",
    "            params.update(data[\"continue\"])\n",
    "        else:\n",
    "            break\n",
    "    return total\n",
    "\n",
    "def get_wikipedia_text(title: str, intro_only: bool = True) -> Dict[str, str]:\n",
    "    \"\"\"\n",
    "    Fetch the summary (and optionally full text) of a Wikipedia article.\n",
    "    Uses the MediaWiki 'extracts' API.\n",
    "\n",
    "    Args:\n",
    "        title: Wikipedia page title\n",
    "        intro_only: If True, return only the summary (intro paragraph).\n",
    "                    If False, return the entire article text.\n",
    "    \"\"\"\n",
    "    if not title:\n",
    "        return {\"summary\": None, \"content\": None}\n",
    "\n",
    "    params = {\n",
    "        \"action\": \"query\",\n",
    "        \"prop\": \"extracts\",\n",
    "        \"titles\": title,\n",
    "        \"format\": \"json\",\n",
    "        \"explaintext\": True,  # plain text instead of HTML\n",
    "    }\n",
    "    if intro_only:\n",
    "        params[\"exintro\"] = True\n",
    "\n",
    "    data = call_api_with_retry(WIKI_API, params)\n",
    "    if not data or \"query\" not in data:\n",
    "        return {\"summary\": None, \"content\": None}\n",
    "\n",
    "    page = next(iter(data[\"query\"][\"pages\"].values()))\n",
    "    text = page.get(\"extract\", \"\")\n",
    "    if intro_only:\n",
    "        return {\"summary\": text, \"content\": None}\n",
    "    else:\n",
    "        # If not intro_only, extract both intro and full content\n",
    "        intro = text.split(\"\\n\")[0] if text else None\n",
    "        return {\"summary\": intro, \"content\": text}\n",
    "\n",
    "\n",
    "def get_all_metrics(title: str) -> Dict[str, Any]:\n",
    "    \"\"\"Return a unified metrics dict for a given English-Wikipedia title.\"\"\"\n",
    "    text_data = get_wikipedia_text(title, intro_only=True)\n",
    "    return {\n",
    "        \"title\": title,\n",
    "        \"wikipedia_pageviews_90d\": get_pageviews_90d(title),\n",
    "        **get_wikipedia_structural_metrics(title),\n",
    "        \"wikipedia_backlinks\": count_backlinks(title),\n",
    "        \"summary\": text_data[\"summary\"],\n",
    "        \"content\": text_data[\"content\"],\n",
    "    }\n",
    "\n",
    "\n",
    "metrics = get_all_metrics(\"Agentic AI\")   # try any title\n",
    "print(json.dumps(metrics, indent=2))\n",
    "\n"
   ]
