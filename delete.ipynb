    "# Notebook-ready: run this whole cell\n",
    "\n",
    "import requests, time, math\n",
    "from datetime import datetime, timedelta, timezone\n",
    "from typing import Any, Dict, List, Optional\n",
    "\n",
    "WIKIDATA_API = \"https://www.wikidata.org/w/api.php\"\n",
    "WIKIPEDIA_API = \"https://en.wikipedia.org/w/api.php\"\n",
    "WDQS_SPARQL = \"https://query.wikidata.org/sparql\"\n",
    "WIKIMEDIA_REST = \"https://wikimedia.org/api/rest_v1\"\n",
    "\n",
    "HEADERS = {\"User-Agent\": \"AgenticAI-Metrics/1.0 (contact: you@example.com)\"}\n",
    "REQUEST_DELAY = 0.2\n",
    "MAX_RETRIES = 3\n",
    "\n",
    "def _get(url: str, params: Dict[str, Any], is_rest: bool=False) -> Optional[Dict[str, Any]]:\n",
    "    \"\"\"GET with retries + JSON safety; `is_rest=True` for REST endpoints that ignore params.\"\"\"\n",
    "    for attempt in range(MAX_RETRIES):\n",
    "        try:\n",
    "            time.sleep(REQUEST_DELAY)\n",
    "            if is_rest:\n",
    "                r = requests.get(url, headers=HEADERS)\n",
    "            else:\n",
    "                r = requests.get(url, params=params, headers=HEADERS)\n",
    "            r.raise_for_status()\n",
    "            txt = r.text.strip()\n",
    "            if not txt:\n",
    "                return None\n",
    "            return r.json()\n",
    "        except Exception as e:\n",
    "            if attempt == MAX_RETRIES - 1:\n",
    "                print(f\"[WARN] GET failed for {url}: {e}\")\n",
    "                return None\n",
    "            time.sleep(2 ** attempt)\n",
    "\n",
    "def _paged_list(api: str, base_params: Dict[str, Any], list_key_path: List[str]) -> List[Any]:\n",
    "    \"\"\"Generic continuation handler for list/prop modules that return 'continue'.\"\"\"\n",
    "    params = dict(base_params)\n",
    "    out: List[Any] = []\n",
    "    while True:\n",
    "        data = _get(api, params)\n",
    "        if not data:\n",
    "            break\n",
    "        node = data\n",
    "        for k in list_key_path:\n",
    "            node = node.get(k, {})\n",
    "        if isinstance(node, list):\n",
    "            out.extend(node)\n",
    "        if \"continue\" in data:\n",
    "            params.update(data[\"continue\"])\n",
    "        else:\n",
    "            break\n",
    "    return out\n",
    "\n",
    "# ---------- Wikidata helpers ----------\n",
    "\n",
    "def get_enwiki_title_from_qid(qid: str) -> Optional[str]:\n",
    "    \"\"\"Resolve English Wikipedia sitelink title from QID.\"\"\"\n",
    "    params = {\"action\":\"wbgetentities\", \"ids\": qid, \"props\":\"sitelinks\", \"format\":\"json\"}\n",
    "    data = _get(WIKIDATA_API, params)\n",
    "    try:\n",
    "        return data[\"entities\"][qid][\"sitelinks\"][\"enwiki\"][\"title\"]\n",
    "    except Exception:\n",
    "        return None\n",
    "\n",
    "def wikidata_entity_age_days(qid: str) -> Optional[int]:\n",
    "    \"\"\"Age from first revision timestamp on wikidata.org item page.\"\"\"\n",
    "    params = {\n",
    "        \"action\":\"query\", \"prop\":\"revisions\", \"titles\": qid,\n",
    "        \"rvlimit\": 1, \"rvdir\": \"newer\", \"rvprop\": \"timestamp\", \"format\":\"json\"\n",
    "    }\n",
    "    data = _get(WIKIDATA_API, params)\n",
    "    if not data: \n",
    "        return None\n",
    "    page = next(iter(data.get(\"query\",{}).get(\"pages\",{}).values()), {})\n",
    "    revs = page.get(\"revisions\", [])\n",
    "    if not revs: \n",
    "        return None\n",
    "    first_ts = revs[0][\"timestamp\"]  # e.g., \"2013-02-14T12:34:56Z\"\n",
    "    t0 = datetime.fromisoformat(first_ts.replace(\"Z\",\"+00:00\"))\n",
    "    days = (datetime.now(timezone.utc) - t0).days\n",
    "    return max(0, days)\n",
    "\n",
    "def wikidata_entity_claims(qid: str) -> Dict[str, Any]:\n",
    "    \"\"\"claims + sitelinks + P18 presence, qualifier count, statement count.\"\"\"\n",
    "    params = {\n",
    "        \"action\":\"wbgetentities\", \"ids\": qid, \"props\":\"claims|sitelinks\", \"format\":\"json\"\n",
    "    }\n",
    "    data = _get(WIKIDATA_API, params)\n",
    "    if not data: \n",
    "        return {}\n",
    "    ent = data[\"entities\"].get(qid, {})\n",
    "    claims = ent.get(\"claims\", {})\n",
    "    # total statements = sum of list lengths across properties\n",
    "    statement_count = sum(len(lst) for lst in claims.values())\n",
    "    # qualifiers: sum the number of qualifier-snaks across all statements\n",
    "    qualifier_count = 0\n",
    "    for lst in claims.values():\n",
    "        for st in lst:\n",
    "            qs = st.get(\"qualifiers\", {})\n",
    "            qualifier_count += sum(len(v) for v in qs.values())\n",
    "    has_img = \"P18\" in claims\n",
    "    language_editions = len(ent.get(\"sitelinks\", {}))\n",
    "    return {\n",
    "        \"statement_count\": statement_count,\n",
    "        \"qualifier_count\": qualifier_count,\n",
    "        \"has_wikidata_image\": has_img,\n",
    "        \"language_editions\": language_editions,\n",
    "    }\n",
    "\n",
    "def wikidata_incoming_links(qid: str, threshold: int = 50000) -> int:\n",
    "    \"\"\"Count pages linking to the item (Special:WhatLinksHere equivalent).\"\"\"\n",
    "    params = {\n",
    "        \"action\":\"query\", \"prop\":\"linkshere\", \"titles\": qid,\n",
    "        \"lhlimit\":\"max\", \"format\":\"json\"\n",
    "    }\n",
    "    total = 0\n",
    "    while True:\n",
    "        data = _get(WIKIDATA_API, params)\n",
    "        if not data: break\n",
    "        for page in data.get(\"query\",{}).get(\"pages\",{}).values():\n",
    "            links = page.get(\"linkshere\", [])\n",
    "            total += len(links)\n",
    "            if total >= threshold:\n",
    "                return total\n",
    "        if \"continue\" in data:\n",
    "            params.update(data[\"continue\"])\n",
    "        else:\n",
    "            break\n",
    "    return total\n",
    "\n",
    "def wikidata_outgoing_links_distinct_properties(qid: str) -> Optional[int]:\n",
    "    \"\"\"\n",
    "    Count distinct 'wdt:' predicate properties used by the item (SPARQL).\n",
    "    This approximates 'outgoing links / information connectivity'.\n",
    "    \"\"\"\n",
    "    query = f\"\"\"\n",
    "    SELECT (COUNT(DISTINCT ?prop) AS ?count) WHERE {{\n",
    "      VALUES ?item {{ wd:{qid} }}\n",
    "      ?item ?prop ?o .\n",
    "      FILTER(STRSTARTS(STR(?prop), STR(wdt:)))\n",
    "    }}\n",
    "    \"\"\"\n",
    "    r = requests.get(\n",
    "        WDQS_SPARQL,\n",
    "        params={\"query\": query, \"format\": \"json\"},\n",
    "        headers=HEADERS,\n",
    "        timeout=60,\n",
    "    )\n",
    "    try:\n",
    "        r.raise_for_status()\n",
    "        js = r.json()\n",
    "        return int(js[\"results\"][\"bindings\"][0][\"count\"][\"value\"])\n",
    "    except Exception as e:\n",
    "        print(\"[WARN] WDQS failed:\", e)\n",
    "        return None\n",
    "\n",
    "# ---------- Wikipedia helpers ----------\n",
    "\n",
    "def wikipedia_pageviews_90d(title: str) -> Optional[int]:\n",
    "    \"\"\"Sum daily pageviews for last 90 days (enwiki).\"\"\"\n",
    "    if not title: return None\n",
    "    end = datetime.utcnow().date()\n",
    "    start = end - timedelta(days=90)\n",
    "    # REST path requires underscores and domain\n",
    "    path = f\"/metrics/pageviews/per-article/en.wikipedia.org/all-access/all-agents/{title.replace(' ','_')}/daily/{start:%Y%m%d}/{end:%Y%m%d}\"\n",
    "    data = _get(WIKIMEDIA_REST + path, {}, is_rest=True)\n",
    "    if not data or \"items\" not in data: \n",
    "        return None\n",
    "    return sum(int(i.get(\"views\",0)) for i in data[\"items\"])\n",
    "\n",
    "def wikipedia_backlinks(title: str, threshold: int = 100000) -> int:\n",
    "    \"\"\"Count backlinks on English Wikipedia.\"\"\"\n",
    "    if not title: return 0\n",
    "    params = {\n",
    "        \"action\":\"query\", \"list\":\"backlinks\", \"bltitle\": title,\n",
    "        \"bllimit\":\"max\", \"format\":\"json\"\n",
    "    }\n",
    "    total = 0\n",
    "    while True:\n",
    "        data = _get(WIKIPEDIA_API, params)\n",
    "        if not data: break\n",
    "        links = data.get(\"query\",{}).get(\"backlinks\",[])\n",
    "        total += len(links)\n",
    "        if total >= threshold: \n",
    "            return total\n",
    "        if \"continue\" in data:\n",
    "            params.update(data[\"continue\"])\n",
    "        else:\n",
    "            break\n",
    "    return total\n",
    "\n",
    "def wikipedia_structural_counts(title: str) -> Dict[str, Any]:\n",
    "    \"\"\"\n",
    "    article_size_bytes, revision_count, unique_editors, category_count, image_count, external_links\n",
    "    (handles continuation to count all where applicable)\n",
    "    \"\"\"\n",
    "    if not title: \n",
    "        return {k: None for k in [\"article_size_bytes\",\"revision_count\",\"unique_editors\",\"category_count\",\"image_count\",\"external_links\"]}\n",
    "    # info (length)\n",
    "    info = _get(WIKIPEDIA_API, {\n",
    "        \"action\":\"query\",\"prop\":\"info\",\"titles\":title,\"format\":\"json\"\n",
    "    })\n",
    "    page = next(iter(info.get(\"query\",{}).get(\"pages\",{}).values()), {}) if info else {}\n",
    "    size = page.get(\"length\")\n",
    "\n",
    "    # revisions (count + unique editors) – paginate\n",
    "    revs = _paged_list(WIKIPEDIA_API, {\n",
    "        \"action\":\"query\",\"prop\":\"revisions\",\"titles\":title,\n",
    "        \"rvprop\":\"user\",\"rvlimit\":\"max\",\"format\":\"json\"\n",
    "    }, list_key_path=[\"query\",\"pages\", next(iter(info.get(\"query\",{}).get(\"pages\",{}).keys()), None) or \"\",\"revisions\"] if info else [\"query\"])\n",
    "    # Fallback if the path trick fails (some APIs return per-page; above covers usual case)\n",
    "    if isinstance(revs, dict): revs = []\n",
    "    unique_users = set(r.get(\"user\") for r in revs if isinstance(r, dict) and \"user\" in r)\n",
    "\n",
    "    # contributors (more reliable for uniques; paginate)\n",
    "    contribs = _paged_list(WIKIPEDIA_API, {\n",
    "        \"action\":\"query\",\"prop\":\"contributors\",\"titles\":title,\n",
    "        \"pclimit\":\"max\",\"format\":\"json\"\n",
    "    }, list_key_path=[\"query\",\"pages\", next(iter(info.get(\"query\",{}).get(\"pages\",{}).keys()), None) or \"\",\"contributors\"] if info else [\"query\"])\n",
    "    if isinstance(contribs, dict): contribs = []\n",
    "    unique_contribs = set(c.get(\"name\") or c.get(\"userid\") for c in contribs if isinstance(c, dict))\n",
    "\n",
    "    # categories/images/extlinks (paginate and count)\n",
    "    cats = _paged_list(WIKIPEDIA_API, {\n",
    "        \"action\":\"query\",\"prop\":\"categories\",\"titles\":title,\n",
    "        \"cllimit\":\"max\",\"format\":\"json\"\n",
    "    }, list_key_path=[\"query\",\"pages\", next(iter(info.get(\"query\",{}).get(\"pages\",{}).keys()), None) or \"\",\"categories\"] if info else [\"query\"])\n",
    "    imgs = _paged_list(WIKIPEDIA_API, {\n",
    "        \"action\":\"query\",\"prop\":\"images\",\"titles\":title,\n",
    "        \"imlimit\":\"max\",\"format\":\"json\"\n",
    "    }, list_key_path=[\"query\",\"pages\", next(iter(info.get(\"query\",{}).get(\"pages\",{}).keys()), None) or \"\",\"images\"] if info else [\"query\"])\n",
    "    exts = _paged_list(WIKIPEDIA_API, {\n",
    "        \"action\":\"query\",\"prop\":\"extlinks\",\"titles\":title,\n",
    "        \"ellimit\":\"max\",\"format\":\"json\"\n",
    "    }, list_key_path=[\"query\",\"pages\", next(iter(info.get(\"query\",{}).get(\"pages\",{}).keys()), None) or \"\",\"extlinks\"] if info else [\"query\"])\n",
    "\n",
    "    return {\n",
    "        \"article_size_bytes\": size,\n",
    "        \"revision_count\": len(revs),\n",
    "        \"unique_editors\": max(len(unique_users), len(unique_contribs)),\n",
    "        \"category_count\": len(cats),\n",
    "        \"image_count\": len(imgs),\n",
    "        \"external_links\": len(exts),\n",
    "    }\n",
    "\n",
    "def wikipedia_text(title: str, include_full_content: bool = True) -> Dict[str, Optional[str]]:\n",
    "    \"\"\"Get summary (intro) and optionally full plain-text content.\"\"\"\n",
    "    if not title:\n",
    "        return {\"summary\": None, \"content\": None}\n",
    "    params = {\n",
    "        \"action\":\"query\", \"prop\":\"extracts\", \"titles\": title,\n",
    "        \"format\":\"json\", \"explaintext\": 1\n",
    "    }\n",
    "    if True:  # always want summary; we’ll get full below if requested\n",
    "        params[\"exintro\"] = 1\n",
    "    data = _get(WIKIPEDIA_API, params)\n",
    "    page = next(iter(data.get(\"query\",{}).get(\"pages\",{}).values()), {}) if data else {}\n",
    "    summary = page.get(\"extract\")\n",
    "\n",
    "    content = None\n",
    "    if include_full_content:\n",
    "        params2 = {\"action\":\"query\",\"prop\":\"extracts\",\"titles\": title,\"format\":\"json\",\"explaintext\":1}\n",
    "        full = _get(WIKIPEDIA_API, params2)\n",
    "        page2 = next(iter(full.get(\"query\",{}).get(\"pages\",{}).values()), {}) if full else {}\n",
    "        content = page2.get(\"extract\")\n",
    "\n",
    "    return {\"summary\": summary, \"content\": content}\n",
    "\n",
    "def wikipedia_reference_count(title: str) -> Optional[int]:\n",
    "    \"\"\"Count <ref> tags via parsed HTML (cheap proxy for references).\"\"\"\n",
    "    if not title: return None\n",
    "    params = {\"action\":\"parse\",\"page\": title,\"prop\":\"text\",\"format\":\"json\"}\n",
    "    data = _get(WIKIPEDIA_API, params)\n",
    "    try:\n",
    "        html = data[\"parse\"][\"text\"][\"*\"]\n",
    "        return html.count(\"<ref\")\n",
    "    except Exception:\n",
    "        return None\n",
    "\n",
    "# ---------- Orchestrator ----------\n",
    "\n",
    "def get_entity_profile(qid: str) -> Dict[str, Any]:\n",
    "    \"\"\"\n",
    "    Main entry: returns every column you listed, plus title/summary/content.\n",
    "    Requires a Wikidata QID; resolves English Wikipedia title automatically.\n",
    "    \"\"\"\n",
    "    title = get_enwiki_title_from_qid(qid)\n",
    "    # Wikidata block\n",
    "    wd_age_days = wikidata_entity_age_days(qid)\n",
    "    wd_claims = wikidata_entity_claims(qid)\n",
    "    profile = {\n",
    "        \"Wikidata_ID\": qid,\n",
    "        \"English_Wikipedia_Title\": title,\n",
    "        # Wikidata-based metrics\n",
    "        \"wikidata_incoming_links\": wikidata_incoming_links(qid),\n",
    "        \"wikidata_outgoing_links\": wikidata_outgoing_links_distinct_properties(qid),\n",
    "        \"language_editions\": wd_claims.get(\"language_editions\"),\n",
    "        \"statement_count\": wd_claims.get(\"statement_count\"),\n",
    "        \"has_wikidata_image\": wd_claims.get(\"has_wikidata_image\"),\n",
    "        \"qualifier_count\": wd_claims.get(\"qualifier_count\"),\n",
    "        \"entity_age_days\": wd_age_days,\n",
    "        \"entity_age_years\": (None if wd_age_days is None else round(wd_age_days/365.25, 2)),\n",
    "    }\n",
    "    # Wikipedia block (if we have a title)\n",
    "    if title:\n",
    "        pv90 = wikipedia_pageviews_90d(title)\n",
    "        struct = wikipedia_structural_counts(title)\n",
    "        text = wikipedia_text(title, include_full_content=True)\n",
    "        refs = wikipedia_reference_count(title)\n",
    "        profile.update({\n",
    "            \"wikipedia_pageviews_90d\": pv90,\n",
    "            \"wikipedia_backlinks\": wikipedia_backlinks(title),\n",
    "            **struct,\n",
    "            \"reference_count\": refs,\n",
    "            \"summary\": text[\"summary\"],\n",
    "            \"content\": text[\"content\"],\n",
    "        })\n",
    "    else:\n",
    "        # Still return placeholders to keep schema stable\n",
    "        profile.update({\n",
    "            \"wikipedia_pageviews_90d\": None,\n",
    "            \"wikipedia_backlinks\": None,\n",
    "            \"article_size_bytes\": None,\n",
    "            \"reference_count\": None,\n",
    "            \"revision_count\": None,\n",
    "            \"unique_editors\": None,\n",
    "            \"category_count\": None,\n",
    "            \"image_count\": None,\n",
    "            \"external_links\": None,\n",
    "            \"summary\": None,\n",
    "            \"content\": None,\n",
    "        })\n",
    "    return profile\n",
    "\n",
    "# ---- quick smoke test (change QID as you like) ----\n",
    "print(get_entity_profile(\"Q42\"))   # Douglas Adams\n"
   ]
  },
